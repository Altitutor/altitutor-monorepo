  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:local": "cp .env.local.backup .env.local 2>/dev/null || cp .env.local .env.local.backup && next dev",
    "dev:remote": "cp .env.local .env.local.backup 2>/dev/null && cp .env.remote .env.local && next dev",
    "switch:local": "node scripts/switch-env.js local",
    "switch:remote": "node scripts/switch-env.js remote",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:ui": "vitest --ui",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:types": "npx supabase gen types typescript --local --schema public > src/shared/lib/supabase/database.types.ts",
    "db:types:remote": "npx supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public > src/shared/lib/supabase/database.types.ts",
    "seed:subjects": "npx tsx src/scripts/seed-subjects.ts --url http://localhost:54321 --key \"eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6InNlcnZpY2Vfcm9sZSIsImV4cCI6MTk4MzgxMjk5Nn0.EGIM96RAZx35lJzdJsyH-qQwv8Hdp7fsn3W0YpN81IU\"",
    "check:subjects": "npx tsx src/scripts/check-subjects.ts --url http://localhost:54321 --key \"eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6InNlcnZpY2Vfcm9sZSIsImV4cCI6MTk4MzgxMjk5Nn0.EGIM96RAZx35lJzdJsyH-qQwv8Hdp7fsn3W0YpN81IU\"",
    "seed:remote": "npx tsx src/scripts/seed-remote.ts",
    "check:remote": "npx tsx src/scripts/check-remote.ts"
  },
